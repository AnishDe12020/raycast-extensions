{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "solana-explorer-raycast",
  "title": "Solana Explorer",
  "description": "View Solana Account and Transaction details right from Raycast",
  "icon": "logo.png",
  "author": "AnishDe12020",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "keywords": [
    "solana",
    "explorer",
    "solscan",
    "web3"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "mainnet",
      "title": "Solana Mainnet Explorer",
      "description": "Explore transactions on the mainnet",
      "mode": "view"
    },
    {
      "name": "devnet",
      "title": "Solana Devnet Explorer",
      "description": "Explore transactions on the devnet",
      "mode": "view"
    },
    {
      "name": "testnet",
      "title": "Solana Testnet Explorer",
      "description": "Explore transactions on the testnet",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.41.1",
    "@raycast/utils": "^1.4.10",
    "@solana/web3.js": "^1.66.2",
    "use-debounce": "^8.0.4"
  },
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
